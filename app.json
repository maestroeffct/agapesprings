{
  "expo": {
    "name": "Agapesprings",
    "slug": "Agapesprings",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "agape",
    "resizeModer": "contain",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "remote-notification",
          "audio",
          "remote-notification"
        ],
        "NSPhotoLibraryAddUsageDescription": "This app saves downloaded songs to your media library.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your media library to store downloaded songs.",
        "NSMusicUsageDescription": "This app lets you download and manage music files from OneSound.",
        "NSDocumentsFolderUsageDescription": "Allow Agapesprings to store downloaded music files."
      },
      "bundleIdentifier": "com.maestro-effect.agapesprings"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./android/app/google-services.json",
      "edgeToEdgeEnabled": false,
      "package": "com.maestro_effect.agapesprings",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_AUDIO",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "notification": {
      "icon": "./assets/images/notification.png",
      "androidCollapsedTitle": "Agapesprings"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      "expo-video",
      [
        "expo-media-library",
        {
          "photosPermission": "This app needs access to your media library to store downloaded songs.",
          "savePhotosPermission": "This app saves downloaded songs to your media library."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "675bb1e3-e37f-469a-bb18-a989cd5f0b73"
      }
    },
    "owner": "maestro_effect"
  }
}
